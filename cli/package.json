{
  "name": "@saf-site/cli",
  "version": "0.1.0",
  "description": "CLI tool for managing SAF site content and database",
  "type": "module",
  "bin": {
    "saf-site": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mitre",
    "saf",
    "cli",
    "content-management"
  ],
  "author": "MITRE SAF Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1",
    "pocketbase": "^0.26.5",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/wait-on": "^5.3.4",
    "@vitest/coverage-v8": "^4.0.17",
    "execa": "^9.6.1",
    "interactive-cli-tester": "^0.3.0",
    "tsup": "^8.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "wait-on": "^9.0.3"
  }
}
