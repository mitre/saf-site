{
  "name": "saf-site-vitepress",
  "version": "1.0.0",
  "description": "MITRE SAF documentation site built with VitePress",
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "preview": "vitepress preview docs",
    "studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:dump": "sqlite-diffable dump .data/saf.db diffable/ --all",
    "db:load": "sqlite-diffable load .data/saf.db diffable/",
    "db:import-v4": "tsx scripts/import-from-v4.ts"
  },
  "keywords": [
    "mitre",
    "saf",
    "security",
    "automation",
    "compliance"
  ],
  "author": "MITRE SAF Team",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {
    "@vue/theme": "^2.3.0",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.21.0",
    "vitepress": "^1.6.4"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@libsql/client": "^0.17.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "pocketbase": "^0.26.5",
    "reka-ui": "^2.7.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  }
}
